# Supabase Configuration
# Get from: https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# AI Provider: "gemini" or "openai"
AI_PROVIDER=gemini

# OpenAI Configuration
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-your-openai-api-key-here

# Gemini Configuration
# Get from: https://aistudio.google.com/apikey
GEMINI_API_KEY=your-gemini-api-key-here

# Encryption (used to store admin-provided model API keys in the DB)
# Generate a long random string (do not commit real values)
MODEL_REGISTRY_ENCRYPTION_SECRET=change-me-to-a-long-random-string

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000



# Agent IDs for each difficulty level (we'll create these first)
ELEVENLABS_API_KEY=
ELEVENLABS_AGENT_EASY=
ELEVENLABS_AGENT_MODERATE=
ELEVENLABS_AGENT_MEDIUM=
ELEVENLABS_AGENT_HARD=
